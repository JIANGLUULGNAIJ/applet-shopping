<!--pages/center/center.wxml-->
<view class="user">
    <image src="{{userInfo.avatarUrl}}" bindtap="changeimage" ></image>
    <view> {{userInfo.nickName}}</view>
</view>

<mp-cells ext-class="my-cells" >
        <mp-cell value="完善信息" >
            <mp-icon slot="icon" type="field" icon="me" color=" #14c145" size="{{25}}"></mp-icon>
            <mp-icon slot="footer" type="field" icon="arrow" color=" #14c145" size="{{15}}"></mp-icon>
        </mp-cell>
        <mp-cell value="个人设置" >
            <mp-icon slot="icon" type="field" icon="like" color=" #14c145" size="{{25}}"></mp-icon>
            <mp-icon slot="footer" type="field" icon="arrow" color=" #14c145" size="{{15}}"></mp-icon>
        </mp-cell>
</mp-cells>
